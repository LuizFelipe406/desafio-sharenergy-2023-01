    "dotenv": "^16.0.3",
    "express": "^4.18.2"
  },
  "devDependencies": {
    "@types/chai": "^4.3.4",
    "@types/chai-http": "^4.2.0",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.15",
    "@types/mocha": "^10.0.1",
    "@types/sinon": "^10.0.13",
    "@types/typescript": "^2.0.0",
    "chai": "^4.3.7",
    "chai-http": "^4.3.0",
    "mocha": "^10.2.0",
    "nodemon": "^2.0.20",
    "sinon": "^15.0.1",
